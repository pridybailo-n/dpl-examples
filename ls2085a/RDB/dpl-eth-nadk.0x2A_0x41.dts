		/* NADK Container */
		dprc@4 {
			compatible = "fsl,dprc";
			parent = "dprc@1";
			options = "DPRC_CFG_OPT_SPAWN_ALLOWED", "DPRC_CFG_OPT_ALLOC_ALLOWED";

			resources {
				res@1 {
					type = "mcp";
					num = <0x1>;
					options = <0x1>;
					id_base_align = <0x4>;
				};
			};

			objects {

				/* ------------ DPNIs --------------*/
				obj@100 {
					obj_name = "dpni@6";
				};

				obj@101 {
