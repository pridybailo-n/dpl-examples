	/*****************************************************************
         * Connections
         *****************************************************************/
	connections {
		connection@1{
			endpoint1 = "dpni@6";
			endpoint2 = "dpmac@1";
		};
		connection@2{
			endpoint1 = "dpni@7";
			endpoint2 = "dpmac@2";
