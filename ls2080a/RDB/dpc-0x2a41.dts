/dts-v1/;
/* DPC */
/ {
	resources {
		icid_pools {
			icid_pool@1 {
				num = <100>;
				base_icid = <0>;
			};
		};
	};
	mc_general {
		log {
			mode = "LOG_MODE_ON";
			level = "LOG_LEVEL_WARNING";
		};
		console {
			mode = "CONSOLE_MODE_ON";
			uart_id = <3>;
		};
	};
	controllers {
		qbman {
			/* Transform one 8-WQ channel into four 2-WQ channels */
			wq_ch_conversion = <1>;
		};
	};
	board_info {
		/* For RDB SerDes 0x2a_41 */
		ports {
			/* Cortina PHYs */
			mac@1 {
				link_type = "MAC_LINK_TYPE_FIXED";
			};
			mac@2 {
				link_type = "MAC_LINK_TYPE_FIXED";
			};
			mac@3 {
				link_type = "MAC_LINK_TYPE_FIXED";
			};
			mac@4 {
				link_type = "MAC_LINK_TYPE_FIXED";
			};
			/* AQR PHYs */
			mac@5 {
				link_type = "MAC_LINK_TYPE_PHY";
			};
		};
	};
};
